package paint_strategies;

/**
 * ButterflyPaintStrategy paints a butterfly gif to the GUI
 * @author ammeurer
 *
 */
public class ButterflyPaintStrategy extends UprightImagePaintStrategy {
	
	/**
	 * Link to the file that contains the butterfly gif
	 */
	public ButterflyPaintStrategy(){
		this("images/butterfly.gif", .5);
	}

	public ButterflyPaintStrategy(String filename, double fillFactor) {
		super(filename, fillFactor);
	}
}
