package paint_strategies.shape;

import java.awt.Shape;

/**
 * IShapeFactory is the interface that all the shape factories implement
 * @author ammeurer
 *
 */
public interface IShapeFactory {
	
	/**
	 * Create a shape
	 * @param x width
	 * @param y height
	 * @param xScale scale for width
	 * @param yScale scale for height
	 * @return
	 */
	Shape makeShape(double x, double y, double xScale, double yScale);

}
